example.com {
    root * /path/static
    file_server
    encode zstd gzip

    @static {
        file
        path *.css *.js *.gif *.jpg *.jpeg *.png *.svg *.woff *.woff2
    }

    header @static Cache-Control "public, max-age=31536000, immutable"
    header Cache-Control "public, max-age=3600"
}
